{
  "name": "rntemplate",
  "version": "0.0.1",
  "description": "A React Native template based on Moove It best practices",
  "keywords": [
    "react-native",
    "jest",
    "template",
    "boilerplate"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "android": "react-native run-android --variant=developmentDebug",
    "android:prod": "react-native run-android --variant=productionDebug",
    "android:staging": "react-native run-android --variant=stagingDebug",
    "ios": "react-native run-ios --scheme 'Development'",
    "ios:prod": "react-native run-ios --scheme 'Production'",
    "ios:staging": "react-native run-ios --scheme 'Staging'",
    "clean": "watchman watch-del-all && rm -rf package-lock.json && rm -rf node_modules && npm install",
    "start": "react-native start",
    "test": "jest --watchAll",
    "coverage": "jest --coverage",
    "test:ci": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-masked-view/masked-view": "^0.2.7",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "axios": "^0.27.2",
    "prop-types": "^15.8.1",
    "react": "18.1.0",
    "react-native": "0.70.1",
    "react-native-bootsplash": "^4.3.2",
    "react-native-config": "^1.4.6",
    "react-native-gesture-handler": "^2.6.1",
    "react-native-localization": "^2.3.1",
    "react-native-reanimated": "^2.10.0",
    "react-native-safe-area-context": "^4.3.4",
    "react-native-screens": "^3.11.1",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/runtime": "^7.19.0",
    "@react-native-community/eslint-config": "^3.0.1",
    "@testing-library/jest-native": "^4.0.12",
    "@testing-library/react-native": "^11.1.0",
    "@types/jest": "^29.0.3",
    "babel-jest": "^29.0.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.23.1",
    "eslint-import-resolver-babel-module": "5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-module-resolver": "^1.5.0",
    "eslint-plugin-react-native-a11y": "^3.2.1",
    "eslint-plugin-testing-library": "^5.6.4",
    "jest": "^29.0.3",
    "metro-react-native-babel-preset": "^0.72.3",
    "react-test-renderer": "18.1.0",
    "redux-mock-store": "^1.5.4"
  }
}
